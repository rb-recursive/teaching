Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"FindLines", "[", 
    RowBox[{"doc_", ",", "regex_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "doclines", ",", "linespos", ",", "lines", ",", "casesbyline", ",", 
       "casesoverall"}], "}"}], ",", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Check", " ", "if", " ", "document", " ", "still", " ", "big", " ", 
       "long", " ", "string", " ", "or", " ", "split", " ", "into", " ", 
       "lines", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"in", " ", "latter", " ", "case"}], ",", " ", 
         RowBox[{"do", " ", "the", " ", "splitting"}]}], ")"}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"ListQ", "@", "doc"}], ",", 
        RowBox[{"doclines", "=", "doc"}], ",", 
        RowBox[{"doclines", "=", 
         RowBox[{"StringSplit", "[", 
          RowBox[{"doc", ",", "\"\<\\n\>\""}], "]"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "Quiet", " ", "here", " ", "as", " ", "seems", " ", "to", " ", "work",
          " ", "though", " ", "throws"}], "..."}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
      "(*", "\"\<String or list of strings expected at position 1 in \
StringContainsQ[List,RegularExpression[...]]\>\"", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"linespos", "=", 
       RowBox[{"Quiet", "[", 
        RowBox[{
         RowBox[{"Position", "[", 
          RowBox[{"_", "?", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"StringContainsQ", "[", 
              RowBox[{"#", ",", "regex"}], "]"}], "&"}], ")"}]}], "]"}], "@", 
         "doclines"}], "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Extract", " ", "the", " ", "lines"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"lines", "=", 
       RowBox[{"Extract", "[", 
        RowBox[{"doclines", ",", "linespos"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "Extract", " ", "any", " ", "captures", " ", "in", " ", "each", " ", 
         "line"}], " ", "-", " ", 
        RowBox[{"Empty", " ", "lists", " ", "if", " ", "RegEx", " ", 
         RowBox[{"doesn", "'"}], "t", " ", "contain", " ", "capture"}]}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"casesbyline", "=", 
       RowBox[{
        RowBox[{
         RowBox[{"StringCases", "[", 
          RowBox[{"#", ",", 
           RowBox[{"regex", "\[Rule]", "\"\<$1\>\""}]}], "]"}], "&"}], "/@", 
        "lines"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "Extract", " ", "any", " ", "captures", " ", "in", " ", "whole", " ", 
         "document"}], " ", "-", " ", 
        RowBox[{"Empty", " ", "list", " ", "if", " ", "RegEx", " ", 
         RowBox[{"doesn", "'"}], "t", " ", "contain", " ", "capture"}]}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"casesoverall", "=", 
       RowBox[{"DeleteDuplicates", "[", 
        RowBox[{"Join", "@@", "casesbyline"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"{", 
        RowBox[{
        "linespos", ",", "lines", ",", "casesbyline", ",", "casesoverall"}], 
        "}"}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.78185349641757*^9, 3.7818534996498823`*^9}, {
  3.7818537204781075`*^9, 3.781853754330634*^9}, {3.7818539782028475`*^9, 
